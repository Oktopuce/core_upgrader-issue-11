{
    "name": "wapplersystems/core-upgrader",
    "description": "Run upgrade wizards for multiple TYPO3 versions at once and clean up the system",
    "type": "typo3-cms-extension",
    "keywords": [
        "TYPO3 CMS",
        "Extension",
        "Upgrade",
        "Update",
        "Wizards",
        "maintenance",
        "cleanup"
    ],
    "homepage": "https://github.com/WapplerSystems/core_upgrader",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Sven Wappler",
            "email": "typo3@wappler.systems",
            "role": "Developer"
        },
        {
            "name": "Nicole Cordes",
            "email": "typo3@cordes.co",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0 || ^8.1 || ^8.2",
        "typo3/cms-core": "^12.4.0",
        "typo3/cms-frontend": "^12.4.0",
        "typo3/cms-install": "^12.4.0",
        "helhum/typo3-console": "^8.0.0"
    },
    "autoload": {
        "psr-4": {
            "WapplerSystems\\Upgrader\\": "Classes/",
            "TYPO3\\CMS\\v76\\Install\\Updates\\": "Classes/Updates/v76/",
            "TYPO3\\CMS\\v87\\Install\\Updates\\": "Classes/Updates/v87/",
            "TYPO3\\CMS\\v95\\Install\\Updates\\": "Classes/Updates/v95/",
            "TYPO3\\CMS\\v104\\Install\\Updates\\": "Classes/Updates/v104/",
            "TYPO3\\CMS\\v12\\Install\\Updates\\": "Classes/Updates/v12/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "core_upgrader2"
        }
    }
}
